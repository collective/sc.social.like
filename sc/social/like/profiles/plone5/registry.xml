<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="sc.social.like">

<!-- Bundle resource. This is a file that will define a
     require method(requirejs) and be built -->
  <records prefix="plone.resources/sc-social-like"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
     <value key="css">
        <element>++plone++sl_stylesheets/social_like.css</element>
     </value>
     <value key="js">++plone++sl_scripts/social_like.js</value>
  </records>

  <records prefix="plone.bundles/sc-social-like-bundle"
            interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="resources">
      <element>sc-social-like</element>
    </value>
    <value key="enabled">True</value>
    <value key="jscompilation">++plone++sl_scripts/social_like.js</value>
    <value key="csscompilation">++plone++sl_stylesheets/social_like.css</value>
    <value key="last_compilation">2015-01-26 12:07:27</value>
   </records>

</registry>
